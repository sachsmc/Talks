swag <- function(sd = 1){
Predicted <- rnorm(100)
True <- Predicted + rnorm(100, mean = 0, sd = sd)
Well <- sd
data.frame(Predicted, True, Well)
}
datseet <- do.call(rbind, lapply(c(.1, .5, 1, 1.5, 2), swag))
